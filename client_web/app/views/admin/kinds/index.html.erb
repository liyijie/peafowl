<h1>Listing kinds</h1>

<table>
  <tr>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @kinds.each do |kind| %>
  <tr>
    <td><%= link_to kind.name, admin_kind_path(kind) %></td>
    <!-- <td><%= kind.name %></td> -->
    <td><%= link_to 'Show', admin_kind_path(kind) %></td>
    <td><%= link_to 'Edit', edit_admin_kind_path(kind) %></td>
    <td><%= link_to 'Destroy', admin_kind_path(kind), method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Kind', new_admin_kind_path %>
